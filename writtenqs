e: yes
f: no


private static int totalTVs = 0;

public Television()
{
  supplier = null;
  code = null;
  screenSize = 0;
  type = null;
};

public Television(TelevisionManufacturer supplier, String code, int screenSize, String type)
{
  this.supplier = supplier;
  this.code = code;
  this.screenSize = screenSize;
  this.type = type;
  totalTVs++;
}

public int getScreenSize() { return screenSize; };

public void setType(String type) { this.type = type; };

public boolean isProducedBy(TelevisionManufacturer supplier)
{
  return this.supplier.equals(supplier);
}
